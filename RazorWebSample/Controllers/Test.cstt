/*config
    OutPutFile=.cs
	InputDllFolder=D:\workspace\project\csharp\RazorTransformTool\compilerlibary
	IsHeader=1
*/
@{
    var nameController = "AccountController";
    var listAction = new[] {"ABBC", "CCBBA", "CCCD","EEFF","hell"};
}
@using Microsoft.SqlServer.Management.Common
using System;
using System.Text;
using System.Web;
using System.Web.Mvc;
using System.Web.Routing;
namespace @Model.DefaultNameSpace
{
	
    [Authorize]
    public class @R(nameController) : Controller
    {
        @*AccountController *@
        public @R(nameController) ()
        {
        }

        @foreach (var action in listAction)
        {
            @WF(2,string.Format("public ActionResult {0}()",action))
            @WF(2,"{")
			@WF(3,"return View();");
            @WF(2,"}")
        }
    }
}
